<Window x:Class="TableDataConverter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TableDataConverter"
        mc:Ignorable="d"
        Title="MainWindow" Height="470" Width="400"
        SizeToContent="Manual"
        WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize"
        UseLayoutRounding="True">
    <StackPanel>
        <Button Content="Refresh" HorizontalAlignment="Center" VerticalAlignment="Top" Height="40" Width="208" Click="OnBtn_Refresh"/>
        <Button Content="Confirm" HorizontalAlignment="Center" VerticalAlignment="Top" Height="40" Width="208" Click="OnBtn_Confirm"/>

    </StackPanel>
</Window>
